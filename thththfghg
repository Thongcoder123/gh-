#include <bits/stdc++.h>
using namespace std;

int n, m, t, s, a[1005];

int main()
{
    cin >> n;
    for (int i = 1; i <= n; i++)
    {
        cin >> a[i];
    }
    sort(a + 1, a + n + 1);
    if (n % 2 == 1)
    {
        cout << a[n / 2 + 1];
    }
    else
    {
        cout << a[n / 2];
    }
}
    
